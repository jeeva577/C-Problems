#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    for(int i=0;i<=n;i++){
        for(int j=0;j<=n;j++){
            printf("%d",i);
        }
        printf("\n");
    }
}
#include <stdio.h>

int main()
{
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            printf("%d",(i+j)%5+1);
        }printf("\n");
    }
    
}

